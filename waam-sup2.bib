@article{schwartz_89_predicting,
  title = {Predicting Tree Frequencies from Pollen Frequency: An Attempt to Validate the {{R}} Value Method},
  shorttitle = {Predicting Tree Frequencies from Pollen Frequency},
  author = {Schwartz, Mark W.},
