@article{davies14_improving,
  title = {Improving the Application of Long-Term Ecology in Conservation and Land Management},
  author = {Davies, Althea L. and Colombo, Sergio and Hanley, Nick},
  year = {2014},
  month = feb,
  journal = {Journal of Applied Ecology},
  volume = {51},
  number = {1},
  pages = {63--70},
  issn = {1365-2664},
  doi = {10.1111/1365-2664.12163},
  abstract = {* Significant effort is being made to develop more inclusive and systematic decision-making frameworks in ecology, but these have yet to include palaeoecology. Doing so would address critical questions about long-term ecological processes (spanning {$>$}50~years). * This paper outlines the main barriers to the integration of long-term ecological data (LTE) into management. Using two UK upland case studies, it uses a choice experiment to assess the value placed on LTE by ecological researchers, policymakers and practitioners. Respondents were able to consider how selecting or excluding different sources of evidence might affect management decisions and their environmental outcomes. * The results suggest that LTE has the potential to become a valued part of the evidence base for guiding land-management decisions. * Synthesis and applications. Placing more emphasis on site-based approaches can help translate this potential into practice by demonstrating the practical benefits of using LTE. By working with managers to address site-based issues, palaeoecology can provide additional insights into ecosystem dynamics and critical thresholds. Using LTE can also improve conservation effectiveness by ensuring that both rapid and lagged responses are anticipated and indicating the range of variability against which management responses can be evaluated.},
  language = {en},
  keywords = {_tablet,applied palaeoecology,choice experiments,decision-making framework,evidence base,integrative ecology,Palaeoecology,stated preferences,upland management},
  file = {/Users/oliviaburge/Zotero/storage/2GTKGK8V/Davies et al_2014_Improving the application of long-term ecology in conservation and land.pdf;/Users/oliviaburge/Zotero/storage/DDTEI3G9/Davies et al_2014_Improving the application of long-term ecology in conservation and land.pdf;/Users/oliviaburge/Zotero/storage/HTJJK3TX/Davies et al_2014_Improving the application of long-term ecology in conservation and land.pdf;/Users/oliviaburge/Zotero/storage/M7R66VMU/abstract.xml;/Users/oliviaburge/Zotero/storage/A66IU9TK/abstract.html;/Users/oliviaburge/Zotero/storage/NMCB8XZM/abstract.html}
